              <button onclick="openTargetedCategoriesModal()" class="px-4 py-2 border border-green-600 text-green-600 dark:text-green-400 text-sm font-semibold rounded-xl hover:bg-green-600 hover:text-white transition-all duration-300">
                Eligibility Check
              </button>
            </div>
          </div>
        </div>
      </div>

      <!-- Family Sponsorship - Premium Hero Card -->
      <div id="family-sponsorship" class="mb-16 opacity-0 animate-fade-in-up" style="animation-delay: 1.2s; animation-fill-mode: forwards;">
        <div class="group relative">
          <div class="absolute inset-0 bg-gradient-to-r from-pink-600 via-red-600 to-orange-600 rounded-3xl blur-xl opacity-0 group-hover:opacity-100 transition-all duration-700 scale-110"></div>
          <div class="relative bg-white dark:bg-gray-800 rounded-3xl p-10 shadow-2xl border border-white/20 dark:border-gray-700/50 hover:shadow-3xl transition-all duration-700 transform hover:-translate-y-4 hover:scale-105 group-hover:border-pink-400/50 dark:group-hover:border-pink-500/50 overflow-hidden">
