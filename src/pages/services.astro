---
import Layout from '../layouts/Layout.astro';

export const metadata = {
  title: 'Immigration Services - Express Entry, Study Permit, Work Permit | Immigratic',
  description: 'Discover your path to Canada with our comprehensive immigration services. From Express Entry to Study Permits, we offer expert guidance for every immigration program.',
  image: '/og-services.jpg',
  canonical: 'https://immigratic.com/services'
};
---

<Layout title="Immigration Services - Express Entry, Study Permit, Work Permit | Immigratic">
  <!-- Hero Section - Interactive Program Explorer -->
  <section class="relative min-h-screen bg-gradient-to-br from-blue-900 via-purple-900 to-blue-900 overflow-hidden">
    <!-- Animated Background Elements -->
    <div class="absolute inset-0">
      <div class="absolute top-20 left-20 w-96 h-96 bg-blue-500/10 rounded-full blur-3xl animate-pulse"></div>
      <div class="absolute bottom-20 right-20 w-80 h-80 bg-purple-500/10 rounded-full blur-3xl animate-pulse" style="animation-delay: 2s;"></div>
      <div class="absolute top-1/2 left-1/2 w-64 h-64 bg-green-500/10 rounded-full blur-3xl animate-pulse" style="animation-delay: 4s;"></div>
    </div>
    
    <!-- Floating Navigation Particles -->
    <div class="absolute inset-0 pointer-events-none">
      <div class="floating-nav-particle absolute top-1/4 left-1/4 w-2 h-2 bg-white/30 rounded-full animate-float-gentle"></div>
      <div class="floating-nav-particle absolute top-3/4 right-1/4 w-1.5 h-1.5 bg-blue-300/40 rounded-full animate-float-gentle" style="animation-delay: 1s;"></div>
      <div class="floating-nav-particle absolute top-1/2 left-1/3 w-1 h-1 bg-purple-300/50 rounded-full animate-float-gentle" style="animation-delay: 2s;"></div>
    </div>

    <div class="relative z-10 max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 pt-32 pb-20">
      <!-- Smart Breadcrumbs -->
      <nav class="flex items-baseline justify-center sm:justify-start flex-wrap gap-2 text-sm text-blue-200 mb-8" aria-label="Breadcrumb">
        <a href="/" class="hover:text-white transition-colors duration-200 leading-none">Home</a>
        <svg class="w-4 h-4 text-blue-300 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
          <path fill-rule="evenodd" d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z" clip-rule="evenodd" />
        </svg>
        <span class="text-white font-medium leading-none">Services</span>
      </nav>

      <!-- Hero Content -->
      <div class="text-center mb-16">
        <div class="inline-flex items-center gap-3 mb-8 opacity-0 animate-fade-in-up" style="animation-delay: 0.2s; animation-fill-mode: forwards;">
          <div class="w-3 h-3 bg-gradient-to-r from-blue-400 to-purple-400 rounded-full animate-pulse"></div>
          <span class="text-blue-200 font-semibold text-sm uppercase tracking-wider">Comprehensive Solutions</span>
          <div class="w-3 h-3 bg-gradient-to-r from-purple-400 to-pink-400 rounded-full animate-pulse" style="animation-delay: 1s;"></div>
        </div>
        
        <h1 class="text-5xl md:text-7xl font-bold text-white mb-8 opacity-0 animate-fade-in-up" style="animation-delay: 0.4s; animation-fill-mode: forwards;">
          Your Immigration
          <span class="block bg-gradient-to-r from-blue-400 via-purple-400 to-pink-400 bg-clip-text text-transparent animate-gradient-shift">
            Journey Starts Here
          </span>
        </h1>
        
        <p class="text-xl md:text-2xl text-blue-100 max-w-4xl mx-auto mb-12 opacity-0 animate-fade-in-up" style="animation-delay: 0.6s; animation-fill-mode: forwards;">
          Discover your path to Canada with our revolutionary immigration platform.
          From Express Entry to Study Permits, we offer expert guidance for every immigration program.
        </p>
      </div>

      <!-- Live Immigration Data Stream -->
      <div class="max-w-4xl mx-auto opacity-0 animate-fade-in-up" style="animation-delay: 1s; animation-fill-mode: forwards;">
        <div class="bg-white/5 backdrop-blur-xl rounded-2xl border border-white/10 p-6">
          <div class="flex items-center justify-between mb-4">
            <h3 class="text-lg font-semibold text-white">Live Immigration Updates</h3>
            <div class="flex items-center gap-2">
              <div class="w-2 h-2 bg-green-400 rounded-full animate-pulse"></div>
              <span class="text-sm text-green-300">Live</span>
            </div>
          </div>
          <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
            <div class="text-center">
              <div class="text-2xl font-bold text-blue-300 mb-1">4,750</div>
              <div class="text-sm text-blue-200">Applications Processed</div>
            </div>
            <div class="text-center">
              <div class="text-2xl font-bold text-purple-300 mb-1">96.8%</div>
              <div class="text-sm text-purple-200">Success Rate</div>
            </div>
            <div class="text-center">
              <div class="text-2xl font-bold text-green-300 mb-1">2.4</div>
              <div class="text-sm text-green-200">Avg. Processing (Months)</div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Navigation Menu -->
  <section class="py-16 bg-white dark:bg-gray-900">
    <div class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="flex flex-wrap justify-center gap-8">
        <a href="#permanent-residency" class="text-gray-700 dark:text-gray-300 hover:text-blue-600 dark:hover:text-blue-400 font-medium transition-colors duration-200 flex items-center gap-2">
          <span>🚀</span>
          Permanent Residency
        </a>
        
        <a href="#temporary-residence" class="text-gray-700 dark:text-gray-300 hover:text-teal-600 dark:hover:text-teal-400 font-medium transition-colors duration-200 flex items-center gap-2">
          <span>🎯</span>
          Temporary Residence
        </a>
      </div>
    </div>
  </section>

  <!-- PR Programs Section - Interactive Immigration Pathways -->
  <section id="permanent-residency" class="py-24 bg-gradient-to-br from-gray-50 to-blue-50 dark:from-gray-800 dark:to-gray-900 relative overflow-hidden">
    <!-- Background Elements -->
    <div class="absolute inset-0">
      <div class="absolute top-0 left-0 w-full h-full bg-gradient-to-r from-blue-600/5 to-purple-600/5"></div>
      <div class="absolute top-20 right-20 w-96 h-96 bg-blue-500/5 rounded-full blur-3xl"></div>
    </div>
    
    <div class="relative z-10 max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <!-- Section Header -->
      <div class="text-center mb-20">
        <div class="inline-flex items-center gap-3 mb-6 opacity-0 animate-fade-in-up" style="animation-delay: 0.2s; animation-fill-mode: forwards;">
          <div class="w-3 h-3 bg-gradient-to-r from-blue-500 to-purple-600 rounded-full animate-pulse"></div>
          <span class="text-blue-600 dark:text-blue-400 font-semibold text-sm uppercase tracking-wider">PR</span>
          <div class="w-3 h-3 bg-gradient-to-r from-purple-500 to-pink-600 rounded-full animate-pulse" style="animation-delay: 1s;"></div>
        </div>
        
        <h2 class="text-4xl md:text-6xl font-bold text-gray-900 dark:text-white mb-6 opacity-0 animate-fade-in-up" style="animation-delay: 0.4s; animation-fill-mode: forwards;">
          Your Path to
          <span class="block bg-gradient-to-r from-blue-600 via-purple-600 to-pink-600 bg-clip-text text-transparent animate-gradient-shift">
            PR
          </span>
        </h2>
        
        <p class="text-xl text-gray-600 dark:text-gray-300 max-w-3xl mx-auto opacity-0 animate-fade-in-up" style="animation-delay: 0.6s; animation-fill-mode: forwards;">
          Navigate the complex world of Canadian immigration with our interactive pathways and real-time guidance.
        </p>
      </div>

      <!-- Interactive Program Pathways -->
      <div class="grid grid-cols-1 lg:grid-cols-2 gap-12 mb-16">
        <!-- Express Entry Programs -->
        <div id="express-entry" class="group relative opacity-0 animate-fade-in-up" style="animation-delay: 0.8s; animation-fill-mode: forwards;">
          <div class="absolute inset-0 bg-gradient-to-r from-blue-600 to-purple-600 rounded-3xl blur-xl opacity-0 group-hover:opacity-100 transition-all duration-700 scale-110"></div>
          <div class="relative bg-white dark:bg-gray-800 rounded-3xl p-8 shadow-2xl border border-white/20 dark:border-gray-700/50 hover:shadow-3xl transition-all duration-700 transform hover:-translate-y-4 hover:scale-105 group-hover:border-blue-400/50 dark:group-hover:border-blue-500/50">
            <!-- Program Header -->
            <div class="flex items-center gap-4 mb-6">
              <div class="w-16 h-16 bg-gradient-to-br from-blue-500 to-purple-600 rounded-2xl flex items-center justify-center text-white group-hover:scale-110 transition-transform duration-300">
                <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 24 24">
                  <path d="M13 2.05v3.03c3.39.49 6 3.39 6 6.92 0 .9-.18 1.75-.48 2.54l2.6 1.53c.56-1.24.88-2.62.88-4.07 0-5.18-3.95-9.45-9-9.95zM12 19c-3.87 0-7-3.13-7-7 0-3.53 2.61-6.43 6-6.92V2.05c-5.05.5-9 4.76-9 9.95 0 5.52 4.47 10 9.99 10 3.31 0 6.24-1.61 8.06-4.09l-2.6-1.53C16.17 17.98 14.21 19 12 19z"/>
                  <path d="M12 7l-3.2 9h1.9l.7-2h3.2l.7 2h1.9L12 7zm-1.15 5.65L12 9l1.15 3.65h-2.3z"/>
                </svg>
              </div>
              <div>
                <h3 class="text-2xl font-bold text-gray-900 dark:text-white group-hover:text-blue-600 dark:group-hover:text-blue-400 transition-colors">Express Entry</h3>
                <p class="text-gray-600 dark:text-gray-400">Federal Immigration Programs</p>
              </div>
            </div>
            
            <!-- Program Categories -->
            <div class="mb-6">
              <div class="flex items-center gap-2 mb-3">
                <span class="text-sm font-medium text-gray-700 dark:text-gray-300">Main Programs</span>
                <span class="px-2 py-1 bg-blue-100 dark:bg-blue-900/30 text-blue-700 dark:text-blue-300 text-xs rounded-full">2 Active</span>
              </div>
              <div class="space-y-2">
                <div class="flex items-center gap-2">
                  <div class="w-2 h-2 bg-blue-500 rounded-full"></div>
                  <span class="text-sm text-gray-600 dark:text-gray-400">Canadian Experience Class (CEC)</span>
                </div>
                <div class="flex items-center gap-2">
                  <div class="w-2 h-2 bg-purple-500 rounded-full"></div>
                  <span class="text-sm text-gray-600 dark:text-gray-400">Federal Skilled Worker (FSW)</span>
                </div>
              </div>
            </div>
            
            <!-- Live CRS Score Tracker -->
            <div class="mb-6 p-4 bg-gradient-to-r from-blue-50 to-purple-50 dark:from-blue-900/20 dark:to-purple-900/20 rounded-xl border border-blue-200 dark:border-blue-700">
              <div class="flex items-center justify-between mb-2">
                <span class="text-sm font-medium text-blue-700 dark:text-blue-300">Latest CRS Cutoff</span>
                <span class="text-xs text-blue-600 dark:text-blue-400">Updated 2 hours ago</span>
              </div>
              <div class="text-3xl font-bold text-blue-600 dark:text-blue-400">534</div>
              <div class="text-xs text-blue-600 dark:text-blue-400">Last Draw: August 7, 2025</div>
            </div>
            
            <!-- Quick Actions -->
            <div class="flex gap-3">
              <a href="/services/pr?utm_source=immigratic&utm_medium=website&utm_campaign=services_page&utm_content=express_entry" class="flex-1 px-4 py-2 bg-gradient-to-r from-blue-600 to-purple-600 text-white text-sm font-semibold rounded-xl hover:from-blue-700 hover:to-purple-700 transition-all duration-300 transform hover:scale-105 text-center">
                Explore Programs
              </a>
              <button onclick="openEligibilityModal()" class="px-4 py-2 border border-blue-600 text-blue-600 dark:text-blue-400 text-sm font-semibold rounded-xl hover:bg-blue-600 hover:text-white transition-all duration-300">
                Eligibility Check
              </button>
            </div>
          </div>
        </div>

        <!-- Express Entry Targeted Categories -->
        <div class="group relative opacity-0 animate-fade-in-up" style="animation-delay: 1s; animation-fill-mode: forwards;">
          <div class="absolute inset-0 bg-gradient-to-r from-green-600 to-blue-600 rounded-3xl blur-xl opacity-0 group-hover:opacity-100 transition-all duration-700 scale-110"></div>
          <div class="relative bg-white dark:bg-gray-800 rounded-3xl p-8 shadow-2xl border border-white/20 dark:border-gray-700/50 hover:shadow-3xl transition-all duration-700 transform hover:-translate-y-4 hover:scale-105 group-hover:border-green-400/50 dark:group-hover:border-green-500/50">
            <!-- Program Header -->
            <div class="flex items-center gap-4 mb-6">
              <div class="w-16 h-16 bg-gradient-to-br from-green-500 to-blue-600 rounded-2xl flex items-center justify-center text-white group-hover:scale-110 transition-transform duration-300">
                <svg class="w-8 h-8" fill="currentColor" viewBox="0 0 24 24">
                  <path d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"/>
                </svg>
              </div>
              <div>
                <h3 class="text-2xl font-bold text-gray-900 dark:text-white group-hover:text-green-600 dark:group-hover:text-green-400 transition-colors">Express Entry</h3>
                <p class="text-gray-600 dark:text-gray-400">Federal Priority Streams</p>
              </div>
            </div>
            
            <!-- Targeted Categories -->
            <div class="mb-6">
              <div class="flex items-center gap-2 mb-3">
                <span class="text-sm font-medium text-gray-700 dark:text-gray-300">Priority Categories</span>
                <span class="px-2 py-1 bg-green-100 dark:bg-green-900/30 text-green-700 dark:text-green-300 text-xs rounded-full">6 Active</span>
              </div>
              <div class="grid grid-cols-2 gap-2">
                <div class="flex items-center gap-2">
                  <div class="w-2 h-2 bg-green-500 rounded-full"></div>
                  <span class="text-xs text-gray-600 dark:text-gray-400">Trade</span>
                </div>
                <div class="flex items-center gap-2">
                  <div class="w-2 h-2 bg-blue-500 rounded-full"></div>
                  <span class="text-xs text-gray-600 dark:text-gray-400">STEM</span>
                </div>
                <div class="flex items-center gap-2">
                  <div class="w-2 h-2 bg-green-500 rounded-full"></div>
                  <span class="text-xs text-gray-600 dark:text-gray-400">Health</span>
                </div>
                <div class="flex items-center gap-2">
                  <div class="w-2 h-2 bg-blue-500 rounded-full"></div>
                  <span class="text-xs text-gray-600 dark:text-gray-400">French</span>
                </div>
                <div class="flex items-center gap-2">
                  <div class="w-2 h-2 bg-green-500 rounded-full"></div>
                  <span class="text-xs text-gray-600 dark:text-gray-400">Education</span>
                </div>
                <div class="flex items-center gap-2">
                  <div class="w-2 h-2 bg-blue-500 rounded-full"></div>
                  <span class="text-xs text-gray-600 dark:text-gray-400">Agriculture</span>
                </div>
              </div>
            </div>
            
            <!-- Latest Cutoff Data -->
            <div class="mb-6 p-4 bg-gradient-to-r from-green-50 to-blue-50 dark:from-green-900/20 dark:to-blue-900/20 rounded-xl border border-green-200 dark:border-green-700">
              <div class="flex items-center justify-between mb-2">
                <span class="text-sm font-medium text-green-700 dark:text-green-300">Latest French Stream Cutoff</span>
                <span class="text-xs text-green-600 dark:text-green-400">Updated 1 day ago</span>
              </div>
              <div class="text-2xl font-bold text-green-600 dark:text-green-400 mb-1">CRS 481</div>
              <div class="text-xs text-green-600 dark:text-green-400">Last Draw: August 8, 2025</div>
            </div>
            
            <!-- Quick Actions -->
            <div class="flex gap-3">
              <a href="/services/pr?utm_source=immigratic&utm_medium=website&utm_campaign=services_page&utm_content=targeted_categories" class="flex-1 px-4 py-2 bg-gradient-to-r from-green-600 to-blue-600 text-white text-sm font-semibold rounded-xl hover:from-green-700 hover:to-blue-700 transition-all duration-300 transform hover:scale-105 text-center">
                Explore Categories
