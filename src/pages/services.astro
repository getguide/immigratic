            <div class="absolute top-0 right-0 w-64 h-64 bg-gradient-to-br from-pink-100 to-red-100 dark:from-pink-900/20 dark:to-red-900/20 rounded-full blur-3xl opacity-50"></div>
            
            <div class="relative z-10">
              <!-- Header Section -->
              <div class="flex flex-col lg:flex-row items-start justify-between mb-8 gap-4">
                <div class="flex items-center gap-4 lg:gap-6">
