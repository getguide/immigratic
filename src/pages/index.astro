          <div class="relative mb-8">
            <div class="absolute inset-0 bg-gradient-to-br from-blue-500 to-purple-600 rounded-2xl blur-lg opacity-75 group-hover:opacity-100 transition-opacity duration-500"></div>
            <div class="relative w-20 h-20 bg-gradient-to-br from-blue-500 to-purple-600 rounded-2xl flex items-center justify-center group-hover:scale-110 transition-all duration-500 transform group-hover:rotate-3 group-hover:shadow-2xl">
              <svg class="w-10 h-10 text-white group-hover:scale-110 transition-transform duration-300" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"></path>
              </svg>
              <!-- Icon Glow Effect -->
              <div class="absolute inset-0 bg-gradient-to-br from-blue-400 to-purple-500 rounded-2xl blur-md opacity-0 group-hover:opacity-100 transition-opacity duration-500 -z-10"></div>
            </div>
            <!-- Floating Particles -->
            <div class="absolute -top-2 -right-2 w-3 h-3 bg-blue-400 rounded-full animate-ping opacity-75"></div>
            <div class="absolute -bottom-2 -left-2 w-2 h-2 bg-purple-400 rounded-full animate-ping opacity-75" style="animation-delay: 0.5s;"></div>
          </div>
          
          <h3 class="text-2xl font-bold text-gray-900 dark:text-white mb-4 group-hover:text-blue-600 dark:group-hover:text-blue-400 transition-colors duration-300">
            Permanent Residency
          </h3>
          
          <!-- Clean Interactive List -->
          <div class="space-y-3 mb-6">
            <div class="flex items-center gap-3 hover:translate-x-2 transition-all duration-300 cursor-pointer">
              <div class="w-2 h-2 bg-blue-500 rounded-full hover:scale-150 transition-transform duration-300"></div>
              <span class="text-gray-600 dark:text-gray-300 hover:text-blue-600 dark:hover:text-blue-400 transition-colors duration-300">Express Entry (EE)</span>
            </div>
            
            <div class="flex items-center gap-3 hover:translate-x-2 transition-all duration-300 cursor-pointer" style="transition-delay: 0.1s;">
              <div class="w-2 h-2 bg-purple-500 rounded-full hover:scale-150 transition-transform duration-300"></div>
              <span class="text-gray-600 dark:text-gray-300 hover:text-purple-600 dark:hover:text-purple-400 transition-colors duration-300">Ontario Immigrant Nominee Program</span>
            </div>
            
            <div class="group/item relative">
              <div class="flex items-center gap-3 group-hover/item:translate-x-2 transition-all duration-300 cursor-pointer" style="transition-delay: 0.2s;">
                <div class="w-2 h-2 bg-blue-600 rounded-full group-hover/item:scale-150 transition-transform duration-300"></div>
                <span class="text-gray-600 dark:text-gray-300 group-hover/item:text-blue-600 dark:group-hover:item:text-blue-400 transition-colors duration-300">Family Sponsorship</span>
              </div>

            </div>
          </div>
          
          <!-- Enhanced CTA Button -->
          <div class="group/btn relative">
            <div class="absolute inset-0 bg-gradient-to-r from-blue-600 to-purple-600 rounded-xl blur-lg opacity-0 group-hover/btn:opacity-100 transition-opacity duration-500 scale-110"></div>
