---
import Layout from '../../../layouts/Layout.astro';

export const metadata = {
  title: 'Permanent Residency Programs | Immigratic',
  description: 'Explore Canada\'s top Permanent Residency programs: Express Entry, Provincial Nominee Programs, and Family Sponsorship. Expert guidance for your PR journey.',
  keywords: 'permanent residency, express entry, PNP, family sponsorship, Canada immigration, PR programs',
  openGraph: {
    title: 'Permanent Residency Programs | Immigratic',
    description: 'Explore Canada\'s top Permanent Residency programs: Express Entry, Provincial Nominee Programs, and Family Sponsorship.',
    image: '/og-image-pr.jpg',
    url: 'https://immigratic.com/services/pr'
  }
};
---

<Layout>
  <!-- Hero Section -->
  <section class="relative min-h-screen bg-gradient-to-br from-blue-900 via-purple-900 to-indigo-900 overflow-hidden">
    <!-- Animated Background -->
    <div class="absolute inset-0">
      <div class="absolute inset-0 bg-gradient-to-r from-blue-600/20 to-purple-600/20"></div>
      <div class="absolute top-0 left-0 w-full h-full">
        <div class="absolute top-20 left-20 w-72 h-72 bg-blue-500/10 rounded-full blur-3xl animate-pulse"></div>
        <div class="absolute top-40 right-20 w-96 h-96 bg-purple-500/10 rounded-full blur-3xl animate-pulse" style="animation-delay: 2s;"></div>
        <div class="absolute bottom-20 left-1/3 w-80 h-80 bg-indigo-500/10 rounded-full blur-3xl animate-pulse" style="animation-delay: 4s;"></div>
      </div>
    </div>

    <!-- Floating Navigation Particles -->
    <div class="absolute inset-0 overflow-hidden pointer-events-none">
      <div class="floating-nav-particle absolute top-1/4 left-1/4 w-2 h-2 bg-blue-400 rounded-full opacity-60"></div>
      <div class="floating-nav-particle absolute top-1/3 right-1/3 w-1 h-1 bg-purple-400 rounded-full opacity-40" style="animation-delay: 1s;"></div>
      <div class="floating-nav-particle absolute top-2/3 left-1/2 w-3 h-3 bg-indigo-400 rounded-full opacity-50" style="animation-delay: 2s;"></div>
      <div class="floating-nav-particle absolute bottom-1/4 right-1/4 w-1.5 h-1.5 bg-blue-300 rounded-full opacity-70" style="animation-delay: 3s;"></div>
    </div>

    <!-- Content -->
    <div class="relative z-10 max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 pt-32 pb-20">
      <!-- Breadcrumbs -->
      <nav class="flex items-baseline justify-start text-white/70 mb-8 text-sm sm:text-base">
        <a href="/" class="hover:text-white transition-colors leading-none">Home</a>
        <span class="text-white/50 mx-1 sm:mx-2 leading-none">→</span>
        <a href="/services" class="hover:text-white transition-colors leading-none">Services</a>
        <span class="text-white/50 mx-1 sm:mx-2 leading-none">→</span>
        <span class="text-white font-semibold leading-none">PR</span>
      </nav>

      <!-- Hero Content -->
      <div class="text-center mb-16">
        <div class="inline-flex items-center gap-3 mb-6 opacity-0 animate-fade-in-up" style="animation-delay: 0.2s; animation-fill-mode: forwards;">
          <div class="w-3 h-3 bg-gradient-to-r from-blue-500 to-purple-600 rounded-full animate-pulse"></div>
          <span class="text-blue-400 font-semibold text-sm uppercase tracking-wider">Permanent Residency</span>
          <div class="w-3 h-3 bg-gradient-to-r from-purple-500 to-pink-600 rounded-full animate-pulse" style="animation-delay: 1s;"></div>
        </div>
        
        <h1 class="text-3xl sm:text-4xl md:text-6xl lg:text-7xl font-bold text-white mb-8 opacity-0 animate-fade-in-up" style="animation-delay: 0.4s; animation-fill-mode: forwards;">
          <span class="slide-reveal-text" style="animation-delay: 0.6s;">
            Your Path to Permanent Residency in <span class="text-red-400 animate-bounce">Canada</span>
          </span>
        </h1>
        
        <p class="text-lg sm:text-xl md:text-2xl text-blue-100 max-w-4xl mx-auto opacity-0 animate-fade-in-up px-4 sm:px-0" style="animation-delay: 0.8s; animation-fill-mode: forwards;">
          Navigate the complex world of Canadian immigration with our interactive pathways and real-time guidance.
        </p>
      </div>

      <!-- Live Immigration Data Stream -->
      <div class="max-w-4xl mx-auto opacity-0 animate-fade-in-up" style="animation-delay: 1s; animation-fill-mode: forwards;">
        <div class="bg-white/10 backdrop-blur-lg rounded-2xl border border-white/20 p-6">
          <div class="flex items-center justify-between mb-4">
            <h3 class="text-lg font-semibold text-white">Live Immigration Data</h3>
            <div class="flex items-center gap-2">
              <div class="w-2 h-2 bg-green-400 rounded-full animate-pulse"></div>
              <span class="text-sm text-green-300">Live</span>
            </div>
          </div>
          <div class="grid grid-cols-1 md:grid-cols-3 gap-4">
            <div class="text-center">
              <div class="text-2xl font-bold text-white">534</div>
              <div class="text-sm text-blue-200">Latest CRS Cutoff</div>
            </div>
            <div class="text-center">
              <div class="text-2xl font-bold text-white">91%</div>
              <div class="text-sm text-blue-200">Success Rate</div>
            </div>
            <div class="text-center">
              <div class="text-2xl font-bold text-white">6-12</div>
              <div class="text-sm text-blue-200">Processing Months</div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Floating Page Navigation -->
  <div class="sticky top-4 z-50 max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 mb-8">
    <nav class="bg-white/90 dark:bg-gray-800/90 backdrop-blur-lg rounded-2xl border border-white/20 dark:border-gray-700/50 shadow-2xl p-3 sm:p-4">
      <div class="flex flex-col sm:flex-row items-center justify-center gap-2 sm:gap-4 md:gap-8">
        <a href="#express-entry" class="flex items-center gap-2 px-3 sm:px-4 py-2 rounded-xl hover:bg-blue-50 dark:hover:bg-blue-900/30 transition-all duration-300 group w-full sm:w-auto justify-center">
          <div class="w-2 h-2 bg-blue-500 rounded-full group-hover:scale-150 transition-transform"></div>
          <span class="font-semibold text-gray-700 dark:text-gray-300 group-hover:text-blue-600 dark:group-hover:text-blue-400 text-sm sm:text-base">Express Entry</span>
        </a>
        <a href="#provincial-nominee" class="flex items-center gap-2 px-3 sm:px-4 py-2 rounded-xl hover:bg-green-50 dark:hover:bg-green-900/30 transition-all duration-300 group w-full sm:w-auto justify-center">
          <div class="w-2 h-2 bg-green-500 rounded-full group-hover:scale-150 transition-transform"></div>
          <span class="font-semibold text-gray-700 dark:text-gray-300 group-hover:text-green-600 dark:group-hover:text-green-400 text-sm sm:text-base">Provincial Nominee</span>
