        </a>
      </div>
    </nav>
  </div>

  <!-- Express Entry Section -->
  <section id="express-entry" class="py-24 bg-gradient-to-br from-gray-50 to-blue-50 dark:from-gray-800 dark:to-gray-900 relative overflow-hidden">
