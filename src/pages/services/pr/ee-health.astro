---
import Layout from '../../../layouts/Layout.astro';
import LiveHealthData from '../../../components/LiveHealthData.astro';
import LiveHealthMetrics from '../../../components/LiveHealthMetrics.astro';
import LiveHealthChart from '../../../components/LiveHealthChart.astro';


export const metadata = {
  title: 'Express Entry Health Category | Healthcare Workers | Immigratic',
  description: 'Master the Express Entry Health Category pathway to permanent residency. Interactive eligibility checker, CRS calculator, and expert guidance for healthcare professionals.',
  keywords: 'Express Entry Health Category, healthcare workers, nurses, doctors, medical professionals, Express Entry, permanent residency, CRS score, immigration',
  openGraph: {
    title: 'Express Entry Health Category | Healthcare Workers | Immigratic',
    description: 'Master the Express Entry Health Category pathway to permanent residency. Interactive tools and expert guidance.',
    image: '/og-image-health.jpg',
    url: 'https://immigratic.com/services/pr/ee-health'
  }
};
---

<Layout title="Express Entry Health Category | Healthcare Workers | Immigratic">
  <!-- Dynamic Hero Section -->
  <section class="relative min-h-screen bg-gradient-to-br from-blue-900 via-indigo-900 to-purple-900 overflow-hidden">
    <!-- Animated Background Elements -->
    <div class="absolute inset-0">
      <div class="absolute inset-0 bg-gradient-to-r from-blue-600/10 to-purple-600/10"></div>
      <!-- Floating Canadian Flag Elements -->
      <div class="absolute top-20 left-10 w-4 h-4 bg-red-500 rounded-full animate-float opacity-60"></div>
      <div class="absolute top-32 right-20 w-3 h-3 bg-red-500 rounded-full animate-float opacity-40" style="animation-delay: 1s;"></div>
      <div class="absolute bottom-40 left-1/4 w-5 h-5 bg-red-500 rounded-full animate-float opacity-50" style="animation-delay: 2s;"></div>
      <!-- Geometric Patterns - Repositioned to avoid content overlap -->
      <div class="absolute top-20 right-10 w-24 h-24 border border-white/5 rounded-3xl rotate-45 animate-pulse"></div>
      <div class="absolute bottom-20 left-10 w-20 h-20 border border-blue-400/10 rounded-2xl rotate-12 animate-pulse" style="animation-delay: 1.5s;"></div>
    </div>

    <!-- Content -->
    <div class="relative z-10 max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 pt-32 pb-20">


      <!-- Hero Content -->
      <div class="text-center mb-16" x-data="{ activeTab: 'overview', eligibilityScore: 0 }">
        <!-- Program Badge -->
        <div class="inline-flex items-center gap-3 mb-6 opacity-0 animate-fade-in-up" style="animation-delay: 0.2s; animation-fill-mode: forwards;">
          <div class="w-3 h-3 bg-gradient-to-r from-red-500 to-red-600 rounded-full animate-pulse"></div>
          <span class="text-red-400 font-semibold text-sm uppercase tracking-wider">Express Entry Program</span>
          <div class="w-3 h-3 bg-gradient-to-r from-red-500 to-red-600 rounded-full animate-pulse" style="animation-delay: 1s;"></div>
        </div>
        
        <!-- Main Title with Typing Effect -->
        <h1 class="text-4xl sm:text-5xl md:text-7xl lg:text-8xl font-bold text-white mb-8 opacity-0 animate-fade-in-up" style="animation-delay: 0.4s; animation-fill-mode: forwards;">
          <span class="block">Express Entry</span>
          <span class="block bg-gradient-to-r from-red-400 via-white to-red-400 bg-clip-text text-transparent">Health</span>
          <span class="block">Category</span>
        </h1>
        
        <!-- Dynamic Subtitle -->
        <p class="text-xl sm:text-2xl text-blue-100 max-w-4xl mx-auto opacity-0 animate-fade-in-up mb-12" style="animation-delay: 0.6s; animation-fill-mode: forwards;">
          Your healthcare expertise is Canada's <strong class="text-yellow-400">priority pathway</strong> to permanent residency
        </p>

        <!-- Interactive Program Selector -->
        <div class="max-w-4xl mx-auto opacity-0 animate-fade-in-up" style="animation-delay: 0.8s; animation-fill-mode: forwards;">
          <div class="bg-white/10 backdrop-blur-lg rounded-3xl border border-white/20 p-8 mb-12">
            <!-- Tab Navigation -->
            <div class="flex justify-center mb-8">
              <div class="bg-white/5 backdrop-blur-sm rounded-2xl p-2 border border-white/20 w-full max-w-lg">
                <div class="grid grid-cols-3 gap-1">
                  <button 
                    @click="activeTab = 'overview'"
                    :class="activeTab === 'overview' ? 'bg-white text-blue-900 shadow-lg' : 'text-white/80 hover:text-white'"
                    class="px-3 sm:px-6 py-3 rounded-xl font-semibold transition-all duration-300 text-sm sm:text-base"
                  >
                    <span class="block sm:hidden">📊</span>
                    <span class="hidden sm:block">📊 Overview</span>
                  </button>
                  <button 
                    @click="activeTab = 'eligibility'"
                    :class="activeTab === 'eligibility' ? 'bg-white text-blue-900 shadow-lg' : 'text-white/80 hover:text-white'"
                    class="px-3 sm:px-6 py-3 rounded-xl font-semibold transition-all duration-300 text-sm sm:text-base"
                  >
                    <span class="block sm:hidden">✅</span>
                    <span class="hidden sm:block">✅ Requirements</span>
                  </button>
                  <button 
                    @click="activeTab = 'advantages'"
                    :class="activeTab === 'advantages' ? 'bg-white text-blue-900 shadow-lg' : 'text-white/80 hover:text-white'"
                    class="px-3 sm:px-6 py-3 rounded-xl font-semibold transition-all duration-300 text-sm sm:text-base"
                  >
                    <span class="block sm:hidden">🚀</span>
                    <span class="hidden sm:block">🚀 Advantages</span>
                  </button>
                </div>
              </div>
            </div>

            <!-- Overview Tab -->
            <div x-show="activeTab === 'overview'" x-transition:enter="transition ease-out duration-500" x-transition:enter-start="opacity-0 transform scale-95" x-transition:enter-end="opacity-100 transform scale-100">
              <div class="grid grid-cols-1 md:grid-cols-3 gap-6 mb-8">
                <div class="text-center">
                  <div class="w-16 h-16 bg-gradient-to-br from-green-400 to-emerald-500 rounded-2xl flex items-center justify-center mx-auto mb-4">
                    <span class="text-2xl">🏥</span>
                  </div>
                  <h3 class="font-bold text-white text-lg mb-2">Healthcare Priority</h3>
                  <p class="text-gray-600 text-sm">Dedicated pathway for medical professionals</p>
                </div>
                <div class="text-center">
                  <div class="w-16 h-16 bg-gradient-to-br from-blue-400 to-indigo-500 rounded-2xl flex items-center justify-center mx-auto mb-4">
                    <span class="text-2xl">⚡</span>
                  </div>
                  <h3 class="font-bold text-white text-lg mb-2">Faster Processing</h3>
                  <p class="text-gray-600 text-sm">Priority processing for healthcare workers</p>
                </div>
                <div class="text-center">
                  <div class="w-16 h-16 bg-gradient-to-br from-purple-400 to-pink-500 rounded-2xl flex items-center justify-center mx-auto mb-4">
                    <span class="text-2xl">📈</span>
                  </div>
                  <h3 class="font-bold text-white text-lg mb-2">Lower CRS Requirements</h3>
                  <p class="text-gray-600 text-sm">Healthcare professionals get competitive advantage</p>
                </div>
              </div>
            </div>

            <!-- Quick Check Tab -->
            <div x-show="activeTab === 'eligibility'" x-transition:enter="transition ease-out duration-500" x-transition:enter-start="opacity-0 transform scale-95" x-transition:enter-end="opacity-100 transform scale-100">
              <div class="text-center">
                <h3 class="text-2xl font-bold text-white mb-4">Are You Health Category Eligible?</h3>
                <p class="text-blue-200 mb-8">Quick checklist to see if you meet the healthcare category requirements</p>
                
                <div class="grid grid-cols-1 md:grid-cols-2 gap-6 mb-8">
                  <div class="bg-white/10 rounded-2xl p-6 border border-white/20">
                    <div class="flex items-center gap-3 mb-4">
                      <span class="text-2xl">👨‍⚕️</span>
                      <h4 class="font-bold text-white">Healthcare Experience</h4>
                    </div>
                    <div class="space-y-2 text-left">
                      <div class="flex items-center gap-2">
                        <span class="text-green-400">✓</span>
                        <span class="text-blue-200 text-sm">Healthcare occupation (NOC 31, 32, 33)</span>
                      </div>
                      <div class="flex items-center gap-2">
                        <span class="text-green-400">✓</span>
                        <span class="text-blue-200 text-sm">6+ months work experience in past 3 years</span>
                      </div>
                      <div class="flex items-center gap-2">
                        <span class="text-green-400">✓</span>
                        <span class="text-blue-200 text-sm">Professional licensing eligibility</span>
                      </div>
                    </div>
                  </div>

                  <div class="bg-white/10 rounded-2xl p-6 border border-white/20">
                    <div class="flex items-center gap-3 mb-4">
                      <span class="text-2xl">🗣️</span>
                      <h4 class="font-bold text-white">Language Skills</h4>
                    </div>
                    <div class="space-y-2 text-left">
                      <div class="flex items-center gap-2">
                        <span class="text-yellow-400">📋</span>
                        <span class="text-blue-200 text-sm">CLB 7 minimum (English/French)</span>
                      </div>
                      <div class="flex items-center gap-2">
                        <span class="text-green-400">✓</span>
                        <span class="text-blue-200 text-sm">Valid language test results</span>
                      </div>
                      <div class="flex items-center gap-2">
                        <span class="text-blue-400">📝</span>
                        <span class="text-blue-200 text-sm">Professional communication skills</span>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- Advantages Tab -->
            <div x-show="activeTab === 'advantages'" x-transition:enter="transition ease-out duration-500" x-transition:enter-start="opacity-0 transform scale-95" x-transition:enter-end="opacity-100 transform scale-100">
              <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
                <div class="space-y-4">
                  <div class="flex items-start gap-4">
                    <div class="w-8 h-8 bg-green-500 rounded-full flex items-center justify-center flex-shrink-0 mt-1">
                      <span class="text-white text-sm">✓</span>
                    </div>
                    <div>
                      <h4 class="font-semibold text-white">Priority Processing</h4>
                      <p class="text-gray-600 text-sm">Healthcare professionals get expedited processing</p>
                    </div>
                  </div>
                  <div class="flex items-start gap-4">
                    <div class="w-8 h-8 bg-green-500 rounded-full flex items-center justify-center flex-shrink-0 mt-1">
                      <span class="text-white text-sm">✓</span>
                    </div>
                    <div>
                      <h4 class="font-semibold text-white">Lower CRS Requirements</h4>
                      <p class="text-gray-600 text-sm">Healthcare category has reduced score thresholds</p>
                    </div>
                  </div>
                </div>
                <div class="space-y-4">
                  <div class="flex items-start gap-4">
                    <div class="w-8 h-8 bg-green-500 rounded-full flex items-center justify-center flex-shrink-0 mt-1">
                      <span class="text-white text-sm">✓</span>
                    </div>
                    <div>
                      <h4 class="font-semibold text-white">Targeted Draws</h4>
                      <p class="text-gray-600 text-sm">Dedicated Express Entry draws for healthcare workers</p>
                    </div>
                  </div>
                  <div class="flex items-start gap-4">
                    <div class="w-8 h-8 bg-green-500 rounded-full flex items-center justify-center flex-shrink-0 mt-1">
                      <span class="text-white text-sm">✓</span>
                    </div>
                    <div>
                      <h4 class="font-semibold text-white">High Demand Profession</h4>
                      <p class="text-gray-600 text-sm">Canada prioritizes healthcare professionals</p>
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <!-- CTA Buttons -->
            <div class="flex flex-col sm:flex-row gap-4 justify-center mt-8 px-4 sm:px-0">
              <a href="https://intake.immigratic.com/ma?utm_source=immigratic&utm_medium=website&utm_campaign=health_page&utm_content=hero_assessment" target="_blank" class="w-full sm:w-auto px-6 sm:px-8 py-4 bg-gradient-to-r from-red-500 to-red-600 text-white font-bold text-center rounded-2xl hover:from-red-600 hover:to-red-700 transition-all duration-300 transform hover:scale-105 shadow-2xl">
                🚀 Free Health Assessment
              </a>
              <a href="https://try.immigratic.com/meetings/immicare/165?utm_source=immigratic&utm_medium=website&utm_campaign=health_page&utm_content=hero_consultation" target="_blank" class="w-full sm:w-auto px-6 sm:px-8 py-4 border-2 border-white/30 text-white font-bold text-center rounded-2xl hover:bg-white/10 transition-all duration-300 backdrop-blur-sm">
                📅 Book Expert Consultation
              </a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Floating Page Navigation -->
  <div class="sticky top-4 z-50 w-full mb-8">
    <nav class="bg-white/90 dark:bg-gray-800/90 backdrop-blur-lg border-t border-b border-white/20 dark:border-gray-700/50 shadow-2xl">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="flex items-center justify-center gap-1 sm:gap-2 md:gap-4 lg:gap-8 py-3 sm:py-4">
          <a href="#eligibility" 
             id="nav-eligibility"
             class="nav-item flex items-center gap-2 px-2 sm:px-3 md:px-4 py-2 rounded-xl transition-all duration-300 group hover:bg-red-50 dark:hover:bg-red-900/30 flex-1 sm:flex-none justify-center">
            <div class="w-2 h-2 bg-red-500 rounded-full group-hover:scale-150 transition-transform"></div>
            <span class="font-semibold text-gray-700 dark:text-gray-300 group-hover:text-red-600 dark:group-hover:text-red-400 text-xs sm:text-sm md:text-base">Eligibility</span>
          </a>
          <a href="#success-stories" 
             id="nav-success-stories"
             class="nav-item flex items-center gap-2 px-2 sm:px-3 md:px-4 py-2 rounded-xl transition-all duration-300 group hover:bg-green-50 dark:hover:bg-green-900/30 flex-1 sm:flex-none justify-center">
            <div class="w-2 h-2 bg-green-500 rounded-full group-hover:scale-150 transition-transform"></div>
            <span class="font-semibold text-gray-700 dark:text-gray-300 group-hover:text-green-600 dark:group-hover:text-green-400 text-xs sm:text-sm md:text-base">Success Stories</span>
          </a>
          <a href="#process" 
             id="nav-process"
             class="nav-item flex items-center gap-2 px-2 sm:px-3 md:px-4 py-2 rounded-xl transition-all duration-300 group hover:bg-blue-50 dark:hover:bg-blue-900/30 flex-1 sm:flex-none justify-center">
            <div class="w-2 h-2 bg-blue-500 rounded-full group-hover:scale-150 transition-transform"></div>
            <span class="font-semibold text-gray-700 dark:text-gray-300 group-hover:text-blue-600 dark:group-hover:text-blue-400 text-xs sm:text-sm md:text-base">Process</span>
          </a>
          <a href="#insights" 
             id="nav-insights"
             class="nav-item flex items-center gap-2 px-2 sm:px-3 md:px-4 py-2 rounded-xl transition-all duration-300 group hover:bg-purple-50 dark:hover:bg-purple-900/30 flex-1 sm:flex-none justify-center">
            <div class="w-2 h-2 bg-purple-500 rounded-full group-hover:scale-150 transition-transform"></div>
            <span class="font-semibold text-gray-700 dark:text-gray-300 group-hover:text-purple-600 dark:group-hover:text-purple-400 text-xs sm:text-sm md:text-base">Insights</span>
          </a>
        </div>
      </div>
    </nav>
  </div>

  <!-- Interactive Eligibility Criteria Section -->
  <section id="eligibility" class="py-24 bg-gradient-to-br from-blue-50 to-indigo-100 dark:from-gray-800 dark:to-gray-900">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <!-- Section Header -->
      <div class="text-center mb-16">
        <div class="inline-flex items-center gap-3 mb-6">
          <div class="w-3 h-3 bg-gradient-to-r from-green-500 to-emerald-600 rounded-full animate-pulse"></div>
          <span class="text-green-600 dark:text-green-400 font-semibold text-sm uppercase tracking-wider">Eligibility Requirements</span>
          <div class="w-3 h-3 bg-gradient-to-r from-emerald-500 to-green-600 rounded-full animate-pulse" style="animation-delay: 1s;"></div>
        </div>
        
        <div class="relative">
          <h2 class="text-4xl md:text-6xl font-bold text-gray-900 dark:text-white mb-6 animate-float-gentle relative z-10">
            Do You Qualify for Health Category?
          </h2>
          <!-- Floating particles around title -->
          <div class="absolute inset-0 pointer-events-none">
            <div class="absolute top-4 left-8 w-2 h-2 bg-blue-400 rounded-full animate-pulse opacity-60"></div>
            <div class="absolute top-8 right-12 w-1 h-1 bg-green-400 rounded-full animate-pulse opacity-60" style="animation-delay: 0.5s;"></div>
            <div class="absolute bottom-8 left-16 w-1.5 h-1.5 bg-purple-400 rounded-full animate-pulse opacity-60" style="animation-delay: 1s;"></div>
            <div class="absolute bottom-4 right-8 w-1 h-1 bg-yellow-400 rounded-full animate-pulse opacity-60" style="animation-delay: 1.5s;"></div>
          </div>
        </div>
        
        <p class="text-xl text-gray-600 dark:text-gray-300 max-w-3xl mx-auto animate-fade-in-up">
          Interactive eligibility checker with real-time feedback on your Health Category application readiness
        </p>
      </div>

      <!-- Comprehensive Eligibility Requirements -->
      <div class="space-y-12">
        <!-- Express Entry Eligibility - FIRST PRIORITY -->
        <div class="eligibility-card group bg-white dark:bg-gray-800 rounded-3xl p-8 shadow-2xl border-2 border-red-200 dark:border-gray-600 transform transition-all duration-700 hover:scale-[1.02] hover:shadow-3xl hover:border-red-300 dark:hover:border-red-500" data-aos="fade-up" data-aos-delay="100">
          <div class="flex items-center gap-4 mb-8">
            <div class="w-16 h-16 bg-gradient-to-br from-red-500 to-pink-600 rounded-3xl flex items-center justify-center animate-float-gentle hover:scale-110 transition-transform duration-300">
              <span class="text-3xl">🚨</span>
            </div>
            <div>
              <h3 class="text-2xl font-bold text-gray-900 dark:text-white group-hover:text-red-600 dark:group-hover:text-red-400 transition-colors duration-300">Express Entry Eligibility</h3>
              <p class="text-gray-600 dark:text-gray-300">Foundation requirement - must qualify first</p>
            </div>
          </div>
          
          <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
            <div class="flex items-start gap-4 bullet-point-item group hover:bg-red-50 dark:hover:bg-red-900/20 p-4 rounded-xl transition-all duration-300">
              <div class="w-8 h-8 bg-red-500 rounded-full flex items-center justify-center flex-shrink-0 mt-1 group-hover:scale-110 group-hover:bg-red-600 transition-all duration-300 animate-pulse-subtle">
                <span class="text-white text-sm">1</span>
              </div>
              <div>
                <h4 class="font-semibold text-gray-900 dark:text-white group-hover:text-red-600 dark:group-hover:text-red-400 transition-colors duration-300">CEC Stream</h4>
                <p class="text-gray-600 dark:text-gray-300 text-sm">Canadian Experience Class - if you have Canadian work experience</p>
                <span class="inline-block px-2 py-1 bg-red-100 dark:bg-red-900/30 text-red-700 dark:text-red-300 text-xs rounded-full mt-2">🇨🇦 Canadian Experience</span>
              </div>
            </div>
            <div class="flex items-start gap-4 bullet-point-item group hover:bg-red-50 dark:hover:bg-red-900/20 p-4 rounded-xl transition-all duration-300">
              <div class="w-8 h-8 bg-red-500 rounded-full flex items-center justify-center flex-shrink-0 mt-1 group-hover:scale-110 group-hover:bg-red-600 transition-all duration-300 animate-pulse-subtle" style="animation-delay: 0.2s;">
                <span class="text-white text-sm">2</span>
              </div>
              <div>
                <h4 class="font-semibold text-gray-900 dark:text-white group-hover:text-red-600 dark:group-hover:text-red-400 transition-colors duration-300">FSW Stream</h4>
                <p class="text-gray-600 dark:text-gray-300 text-sm">Federal Skilled Worker - if you have foreign work experience</p>
                <span class="inline-block px-2 py-1 bg-red-100 dark:bg-red-900/30 text-red-700 dark:text-red-300 text-xs rounded-full mt-2">🌍 Foreign Experience</span>
              </div>
            </div>
            <div class="flex items-start gap-4 bullet-point-item group hover:bg-red-50 dark:hover:bg-red-900/20 p-4 rounded-xl transition-all duration-300">
              <div class="w-8 h-8 bg-red-500 rounded-full flex items-center justify-center flex-shrink-0 mt-1 group-hover:scale-110 group-hover:bg-red-600 transition-all duration-300 animate-pulse-subtle" style="animation-delay: 0.4s;">
                <span class="text-white text-sm">3</span>
              </div>
              <div>
                <h4 class="font-semibold text-gray-900 dark:text-white group-hover:text-red-600 dark:group-hover:text-red-400 transition-colors duration-300">FST Stream</h4>
                <p class="text-gray-600 dark:text-gray-300 text-sm">Federal Skilled Trades - if you have trade qualifications</p>
                <span class="inline-block px-2 py-1 bg-red-100 dark:bg-red-900/30 text-red-700 dark:text-red-300 text-xs rounded-full mt-2">🔧 Trade Skills</span>
              </div>
            </div>
          </div>
        </div>

        <!-- Healthcare Work Experience Requirements -->
        <div class="eligibility-card group bg-white dark:bg-gray-800 rounded-3xl p-8 shadow-2xl border-2 border-blue-200 dark:border-gray-600 transform transition-all duration-700 hover:scale-[1.02] hover:shadow-3xl hover:border-blue-300 dark:hover:border-blue-500" data-aos="fade-up" data-aos-delay="200">
          <div class="flex items-center gap-4 mb-8">
            <div class="w-16 h-16 bg-gradient-to-br from-blue-500 to-indigo-600 rounded-3xl flex items-center justify-center animate-float-gentle hover:scale-110 transition-transform duration-300">
              <span class="text-3xl">🏥</span>
            </div>
            <div>
              <h3 class="text-2xl font-bold text-gray-900 dark:text-white group-hover:text-blue-600 dark:group-hover:text-blue-400 transition-colors duration-300">Healthcare Work Experience</h3>
              <p class="text-gray-600 dark:text-gray-300">Specific requirements for healthcare occupations</p>
            </div>
          </div>
          
          <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
            <div class="space-y-4">
              <div class="flex items-start gap-4 bullet-point-item group hover:bg-blue-50 dark:hover:bg-blue-900/20 p-3 rounded-xl transition-all duration-300">
                <div class="w-8 h-8 bg-green-500 rounded-full flex items-center justify-center flex-shrink-0 mt-1 group-hover:scale-110 group-hover:bg-green-600 transition-all duration-300 animate-pulse-subtle">
                  <span class="text-white text-sm">✓</span>
                </div>
                <div>
                  <h4 class="font-semibold text-gray-900 dark:text-white group-hover:text-blue-600 dark:group-hover:text-blue-400 transition-colors duration-300">Healthcare NOC Codes</h4>
                  <p class="text-gray-600 dark:text-gray-300 text-sm">Must be in healthcare occupations (NOC 31, 32, 33). Your experience can be in multiple NOC categories.</p>
                  <span class="inline-block px-2 py-1 bg-blue-100 dark:bg-blue-900/30 text-blue-700 dark:text-blue-300 text-xs rounded-full mt-2">🏷️ NOC 31, 32, 33</span>
                </div>
              </div>
              <div class="flex items-start gap-4 bullet-point-item group hover:bg-blue-50 dark:hover:bg-blue-900/20 p-3 rounded-xl transition-all duration-300">
                <div class="w-8 h-8 bg-green-500 rounded-full flex items-center justify-center flex-shrink-0 mt-1 group-hover:scale-110 group-hover:bg-green-600 transition-all duration-300 animate-pulse-subtle" style="animation-delay: 0.2s;">
                  <span class="text-white text-sm">✓</span>
                </div>
