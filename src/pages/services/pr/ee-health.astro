<script>
  // Healthcare Occupations Search & Filter Functionality
  document.addEventListener('DOMContentLoaded', function() {
    const searchInput = document.getElementById('occupation-search');
    const teerFilter = document.getElementById('teer-filter');
    const occupationItems = document.querySelectorAll('.occupation-item');
    const occupationCategories = document.querySelectorAll('.occupation-category');

    // Search functionality
    function filterOccupations() {
      const searchTerm = searchInput.value.toLowerCase();
      const selectedTeer = teerFilter.value;
      
      occupationItems.forEach(item => {
        const searchText = item.getAttribute('data-search').toLowerCase();
        const nocCode = item.getAttribute('data-noc');
        const teerLevel = item.getAttribute('data-teer');
        
        // Search by occupation title, NOC code, or TEER level
        const matchesSearch = searchText.includes(searchTerm) || 
                             nocCode.includes(searchTerm) || 
                             `teer ${teerLevel}`.includes(searchTerm) ||
                             `noc ${nocCode}`.includes(searchTerm);
        const matchesTeer = !selectedTeer || teerLevel === selectedTeer;
        
        if (matchesSearch && matchesTeer) {
          item.style.display = 'flex';
          item.style.opacity = '1';
        } else {
          item.style.display = 'none';
          item.style.opacity = '0';
        }
      });

      // Show/hide categories based on visible items
      occupationCategories.forEach(category => {
        const visibleItems = category.querySelectorAll('.occupation-item[style*="display: flex"]');
        if (visibleItems.length > 0) {
          category.style.display = 'block';
        } else {
          category.style.display = 'none';
        }
      });
    }

    // Event listeners
    if (searchInput) {
      searchInput.addEventListener('input', filterOccupations);
    }
    
    if (teerFilter) {
      teerFilter.addEventListener('change', filterOccupations);
    }

    // Add click functionality to occupation items
    occupationItems.forEach(item => {
      item.addEventListener('click', function() {
        const nocCode = this.getAttribute('data-noc');
        const occupationName = this.querySelector('span').textContent;
        
        // Highlight selected item
        occupationItems.forEach(i => i.classList.remove('ring-2', 'ring-emerald-500'));
        this.classList.add('ring-2', 'ring-emerald-500');
        
        // Show NOC code in search bar
        if (searchInput) {
          searchInput.value = `${occupationName} (NOC ${nocCode})`;
          searchInput.focus();
        }
        
        // Scroll to eligibility section
        const eligibilitySection = document.getElementById('eligibility');
        if (eligibilitySection) {
          eligibilitySection.scrollIntoView({ behavior: 'smooth' });
        }
      });
    });

    // Add hover effects and animations
    occupationItems.forEach(item => {
      item.addEventListener('mouseenter', function() {
        this.style.transform = 'scale(1.02)';
        this.style.transition = 'transform 0.2s ease';
      });
      
      item.addEventListener('mouseleave', function() {
        this.style.transform = 'scale(1)';
      });
    });
  });

  // Counter animation for success metrics
  function animateCounters() {
    const counters = document.querySelectorAll('.counter');
    
    counters.forEach(counter => {
      const target = parseFloat(counter.getAttribute('data-target'));
      const decimal = counter.hasAttribute('data-decimal');
      const increment = target / 100;
      let current = 0;
      
      const updateCounter = () => {
        if (current < target) {
          current += increment;
          if (decimal) {
            counter.textContent = current.toFixed(1);
          } else {
            counter.textContent = Math.ceil(current);
          }
          requestAnimationFrame(updateCounter);
        } else {
          if (decimal) {
            counter.textContent = target.toFixed(1);
          } else {
            counter.textContent = target;
          }
        }
      };
      
      updateCounter();
    });
  }

      // Initialize counter animation when page loads
    document.addEventListener('DOMContentLoaded', function() {
      setTimeout(animateCounters, 1000);
    });

    // Quick search function for popular searches
    function quickSearch(term) {
      const searchInput = document.getElementById('occupation-search');
      if (searchInput) {
        searchInput.value = term;
        searchInput.focus();
        // Trigger the search
        const event = new Event('input');
        searchInput.dispatchEvent(event);
      }
    }
</script>

